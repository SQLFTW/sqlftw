includes:
    - ../../vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
    level: 9

    treatPhpDocTypesAsCertain: false

    paths:
        - ../../sources
        - ../../tests

    excludePaths:
        - ../../sources/Reflection/*

    ignoreErrors:

    earlyTerminatingMethodCalls:
        SqlFtw\Parser\TokenList:
            - expected
            - expectedAnyKeyword
